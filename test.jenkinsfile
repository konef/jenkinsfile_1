pipeline {
    agent any
    stages {
        stage ('take') {
            steps {
                git 'https://github.com/konef/hellobash'
            }
        }
        stage ('hello') {
            steps {
                sh label: '', script: 'echo "ass"'
            }
        }
        stage ('create file') {
            steps {
                touch file: 'file.sh', timestamp: 0
            }
        }
        stage ('write') {
            steps {
                sh 'echo "hello" > file.sh'
            }
        }
        stage ('#@#@%$#@') {
            steps {
                sh label: '', script: 'cat file.sh'
            }
        }
    }
}
